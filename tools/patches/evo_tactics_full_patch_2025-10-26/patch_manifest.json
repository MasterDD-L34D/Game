{
  "policy": {
    "parameters_source": "data/evo-tactics/param-synergy",
    "core_docs_source": "modules/evo-tactics/core",
    "monoliths_generated": ["data/species.yaml", "data/packs.yaml", "data/telemetry.yaml", "data/mating.yaml"]
  },
  "sources": {
    "param_zip": "evo_tactics_param_synergy_v8_3.zip",
    "core_zip":  "EvoTactics_FullRepo_v1.0.zip"
  },
  "mapping": [
    {
      "from_zip": "param",
      "include": [
        {"src": "species/*.yaml",         "dst": "data/evo-tactics/param-synergy/species/"},
        {"src": "morph/*.yaml",           "dst": "data/evo-tactics/param-synergy/morph/"},
        {"src": "rules/*.md",             "dst": "data/evo-tactics/param-synergy/rules/"},
        {"src": "rules/*.yaml",           "dst": "data/evo-tactics/param-synergy/rules/"},
        {"src": "tables/*.yaml",          "dst": "data/evo-tactics/param-synergy/tables/"},
        {"src": "telemetry/vc.yaml",      "dst": "data/evo-tactics/param-synergy/telemetry/"},
        {"src": "director/regista_rewards.yaml", "dst": "data/evo-tactics/param-synergy/director/"},
        {"src": "form/mbti_gates.yaml",   "dst": "data/evo-tactics/param-synergy/form/"},
        {"src": "ennea/themes.yaml",      "dst": "data/evo-tactics/param-synergy/ennea/"},
        {"src": "exports/spawn_packs/*.json", "dst": "data/evo-tactics/param-synergy/exports/spawn_packs/"}
      ],
      "exclude": [
        "archive_from_user/_dedup/**"
      ]
    },
    {
      "from_zip": "core",
      "include": [
        {"src": "*.txt", "dst": "modules/evo-tactics/core/"},
        {"src": "telemetry/*.yaml", "dst": "modules/evo-tactics/core/telemetry/"},
        {"src": "scripts/*", "dst": "modules/evo-tactics/core/scripts/"}
      ],
      "exclude": []
    }
  ]
}
