{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Engine Events (Enneagram Hooks)",
  "type": "object",
  "properties": {
    "event": {
      "type": "string",
      "pattern": "^(on_|on[A-Z]).+"
    },
    "aliases": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "payload_shape": {
      "type": "object"
    }
  },
  "required": [
    "event"
  ]
}